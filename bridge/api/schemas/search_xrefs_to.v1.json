{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "search_xrefs_to.v1.json",
  "type": "object",
  "required": ["address", "query", "total_results", "page", "limit", "items"],
  "properties": {
    "address": {
      "type": "string",
      "pattern": "^0x[0-9a-fA-F]+$"
    },
    "query": {
      "type": "string"
    },
    "total_results": {
      "type": "integer",
      "minimum": 0
    },
    "page": {
      "type": "integer",
      "minimum": 0
    },
    "limit": {
      "type": "integer",
      "minimum": 1
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["from_address", "context"],
        "properties": {
          "from_address": {
            "type": "string",
            "pattern": "^0x[0-9a-fA-F]+$"
          },
          "context": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}
