{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "urn:schema:string-xrefs.request.v1",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "string_addr"
  ],
  "properties": {
    "string_addr": {
      "type": "string",
      "pattern": "^0x[0-9a-fA-F]+$"
    },
    "limit": {
      "type": "integer",
      "minimum": 1,
      "maximum": 256
    }
  }
}
