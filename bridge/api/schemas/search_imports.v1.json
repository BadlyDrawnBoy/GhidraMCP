{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "search_imports.v1.json",
  "type": "object",
  "required": ["query", "total_results", "page", "limit", "items"],
  "properties": {
    "query": {
      "type": "string"
    },
    "total_results": {
      "type": "integer",
      "minimum": 0
    },
    "page": {
      "type": "integer",
      "minimum": 0
    },
    "limit": {
      "type": "integer",
      "minimum": 1
    },
    "items": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["name", "address"],
        "properties": {
          "name": {
            "type": "string"
          },
          "address": {
            "type": "string",
            "pattern": "^0x[0-9a-fA-F]+$"
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}
